<%- include('partials/header') %>
<div class="container mx-auto p-4">
    <h1>contact page</h1>
    <form action="/add-home" method="POST" class="bg-gray-200 shadow-md rounded px-8 pt-6 pb-8 mb-4">
      <label class="block text-gray-700 text-sm font-bold mb-2" for="name">Product Title:</label>
      <input class="form-input w-full mb-4" type="text" id="title" name="title" required>
      <label class="block text-gray-700 text-sm font-bold mb-2" for="email">Image Url:</label>
      <input class="form-input w-full mb-4" type="text" id="img_url" name="img_url" required>
      <label class="block text-gray-700 text-sm font-bold mb-2" for="phone">Price:</label>
      <input class="form-input w-full mb-4" type="number" id="price" name="price" required>
      <button type="submit" class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded">Submit</button>
    </form>
    </div>
  </body>
</html>
